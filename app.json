{
  "expo": {
    "name": "app_tapchidhkt",
    "slug": "app_tapchidhkt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "platforms": [
      "android",
      "ios",
      "web"
    ],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "android": {
        "intentFilters": {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.webapp.io",
              "pathPrefix": "/records"
            }
          ],
          "adaptiveIcon": {
            "foregroundImage": "./assets/images/adaptive-icon.png",
            "backgroundColor": "#ffffff"
          },
          "package": "com.tuonganh.app_tapchidhkt",
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        "web": {
          "bundler": "metro",
          "output": "static",
          "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
          "expo-router"
        ],
        "experiments": {
          "typedRoutes": true
        },
        "extra": {
          "eas": {
            "projectId": "13a13aed-d9ac-44f6-a751-ae45e94c156c"
          }
        }
      }
    },
    "plugins": [
      "expo-router"
    ]
  }
}
